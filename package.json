    "mdn-browser-compat-data": "0.0.82",
    "moment": "^2.24.0",
    "mongodb-lock": "^0.4.0",
    "mongoose": "^5.5.13",
    "multiparty": "^4.2.1",
    "serverless-azure-functions": "^0.7.0",
    "strip-bom": "^4.0.0",
    "strip-json-comments": "^3.0.1",
    "tri": "^1.0.2"
  },
  "devDependencies": {
    "@hint/configuration-progressive-web-apps": "^6.0.1",
    "@hint/configuration-web-recommended": "^7.0.1",
    "@hint/hint-https-only": "^2.3.1",
    "@hint/hint-image-optimization-cloudinary": "^3.1.1",
    "@hint/hint-minified-js": "^2.3.1",
    "@hint/hint-no-broken-links": "^4.1.1",
    "@hint/hint-no-p3p": "^3.2.1",
    "@hint/hint-performance-budget": "^2.3.1",
    "@types/azure": "^0.9.20",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.136",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@hint/utils": "^3.1.0",
    "ava": "^2.2.0",
    "cpx": "^1.5.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eventemitter2": "^5.0.1",
    "globby": "^10.0.1",
    "hint": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "postcss": "^7.0.17",
    "proxyquire": "^2.1.1",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Chore: Update `${dependency}` to `v${version}`",
      "devDependencyUpdate": "Chore: Update `${dependency}` to `v${version}`",
      "lockfileUpdate": "Chore: Update lockfile ${lockfilePath}"
    },
    "ignore": [
      "@types/node"
    ]
  },
  "main": "./dist/src/index.js",
  "private": true,
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:* && npm run build-function-templates",
    "build:assets": "cpx \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc",
    "prebuild": "node ./scripts/create-hint-categories.js",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "build-function-templates": "node ./scripts/create-function-templates.js",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch",
    "steps": "npm run step1 && npm run step2",
    "step1": "rimraf node_modules/hint && rimraf gitrepos/ && mkdir gitrepos && cd gitrepos && git clone https://github.com/webhintio/hint.git && cd hint && cd packages && cd hint && npm install && npm install @types/is-ci @types/proxyquire && npm run build",
    "step2": "shx mv gitrepos/hint/packages/hint node_modules/"
  },
  "version": "0.16.0"
}
