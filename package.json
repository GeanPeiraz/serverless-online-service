{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "@azure/functions": "^1.0.3",
    "@destinationstransfers/ntp": "^1.2.0",
    "@octokit/rest": "^16.23.5",
    "applicationinsights": "^1.3.1",
